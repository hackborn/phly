{
	"args": {
		"env": "PHLY_SCALEIMGS_",
		"strings": {
			"files": "./phlib/media/dog.jpg"
		}
	},
	"nodes": {
		"files": {
			"node": "phly/files",
			"cfg": {
				"sep": ";",
				"expand": true,
				"recurse": false
			},
			"ins": {
				"in": "args:files"
			},
			"outs": {
				"out": "batch:in"
			}
		},
		"batch": {
			"node": "phly/batch",
			"cfg": {
				"mode": "items",
				"count": "${cpus}",
				"file": "_scaleimg.json"
			}
		}
	}
}